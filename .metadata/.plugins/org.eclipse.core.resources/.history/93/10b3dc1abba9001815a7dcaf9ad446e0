/**
 * 
 */
function showUserDetails(selectedUser){
	document.getElementById("editForm").style.visibility = "visible";
	document.getElementById("txtUserame").value = selectedUser.username;
	document.getElementById("txtPassword").value = selectedUser.pasword;
	document.getElementById("txtUserId").value = selectedUser.userId;
	document.getElementById("chkCreate").checked = "";
	document.getElementById("chkRead").checked = "";
	document.getElementById("chkUpdate").checked = "";
	document.getElementById("chkDelete").checked = "";
}